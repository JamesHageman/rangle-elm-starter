{
  "name": "react-redux-starter",
  "version": "0.1.0",
  "description": "A boiler-plate setup for React/Redux applications",
  "license": "MIT",
  "engines": {
    "node": "4.4.4"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "clean": "rimraf dist/ coverage/",
    "dev:server": "cross-env NODE_ENV=development PORT=3000 nodemon --use_strict server/node-server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "dev:elm:client": "cross-env NODE_ENV=development webpack-dev-server --port=8081 --config=webpack.elm.config.js -d --inline --hot --progress --no-info",
    "dev:elm": "concurrently -r 'npm run dev:elm:client' 'npm run dev:server'",
    "dev": "concurrently -r 'npm run dev:client' 'npm run dev:server'",
    "lint": "npm run lint-js && npm run lint-css",
    "lint-css": "stylelint './src/**/*.css'",
    "lint-js": "eslint '**/*.js' --ignore-path .gitignore",
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production node --use_strict server/node-server.js",
    "pretest": "npm run clean coverage/ && npm run lint",
    "test": "karma start --singleRun true",
    "posttest": "istanbul check-coverage --statement 75 --branch 60 --function 50",
    "test:watch": "karma start --singleRun false"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "autoprefixer-core": "^6.0.1",
    "babel": "^6.5.2",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-react-constant-elements": "^6.8.0",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.11.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.9.0",
    "basscss": "^8.0.1",
    "basscss-addons": "^1.0.0-beta4",
    "basscss-align": "^1.0.2",
    "basscss-basic": "^1.0.0",
    "basscss-border": "^4.0.2",
    "basscss-flexbox": "^1.0.1",
    "basscss-grid": "^2.0.0",
    "basscss-hide": "^1.0.1",
    "basscss-layout": "^3.1.0",
    "basscss-margin": "^1.0.7",
    "basscss-padding": "^1.1.3",
    "basscss-position": "^2.0.3",
    "basscss-type-scale": "^1.0.5",
    "basscss-typography": "^3.0.3",
    "body-parser": "^1.15.1",
    "classnames": "^2.2.3",
    "concurrently": "^2.0.0",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.0",
    "cssnano": "^3.7.1",
    "denodeify": "^1.2.1",
    "enzyme": "^2.4.1",
    "es5-shim": "^4.5.9",
    "es6-promise": "^3.2.1",
    "es6-shim": "^0.35.1",
    "eslint": "3.0.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-react": "^5.2.2",
    "file-loader": "^0.9.0",
    "history": "^3.0.0",
    "html-webpack-plugin": "^2.9.0",
    "immutable": "^3.7.5",
    "istanbul": "^1.0.0-alpha.2",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^1.1.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.1.0",
    "karma-sinon": "^1.0.5",
    "karma-source-map-support": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "nodemon": "^1.9.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-modules-local-by-default": "^1.0.2",
    "react": "15.2.1",
    "react-addons-test-utils": "15.2.1",
    "react-dom": "15.2.1",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.2",
    "react-router": "^2.4.1",
