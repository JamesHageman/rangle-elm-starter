    "redux-thunk": "^2.0.1",
    "require-hacker": "^2.1.2",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.0",
    "stylelint": "^6.8.0",
    "stylelint-webpack-plugin": "^0.2.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.0",
    "webpack-split-by-path": "0.0.10",
    "whatwg-fetch": "^1.0.0"
  },
  "dependencies": {
    "express": "^4.13.3",
    "helmet": "^2.0.0",
    "http-proxy": "^1.12.1",
    "winston": "^2.1.1"
  },
  "keywords": [
    "react",
    "redux",
    "starter",
    "seed"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rangle/react-redux-starter.git"
  },
  "bugs": {
    "url": "https://github.com/rangle/rangle-starter/issues"
  }
}
